            <param name="quoteNullStrings" value="true"/>
            <param name="timestampAsLong" value="false"/>
            <param name="timestampFormat" value="yyyy-MM-dd HH:mm:ss.SSSZ"/>
            <param name="verbose" value="false"/>
        </box>
        <box name="DelDL1" type="query">
            <input port="1" stream="out:OldLVNodeInfo_1"/>
            <output port="1" stream="out:DelDL1_1"/>
            <dataref id="querytable" name="LVNodeInfoTable"/>
            <param name="operation" value="delete"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="input.URI == current.URI"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="all"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <item name="current" selection="none"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="Iterate" type="iterate">
            <input port="1" stream="out:GetUIRs_1"/>
            <output port="1" stream="out:Iterate_1"/>
            <param name="iterate-expr" value="URIList"/>
            <target-list>
                <item name="input" selection="all"/>
                <expressions>
                    <include field="element">each.element</include>
                </expressions>
            </target-list>
        </box>
        <box name="Iterate2" type="iterate">
            <input port="1" stream="out:ListAllURI_1"/>
            <output port="1" stream="out:Iterate2_1"/>
            <param name="iterate-expr" value="URIList"/>
            <target-list>
                <item name="input" selection="all"/>
                <expressions>
                    <include field="element">each.element</include>
                </expressions>
            </target-list>
        </box>
        <box name="Split" type="split">
            <input port="1" stream="out:NewLVNodeInfo_1"/>
            <output port="1" stream="out:Split_1"/>
            <output port="2" stream="out:Split_2"/>
            <output port="3" stream="out:Split_3"/>
            <output port="4" stream="out:Split_4"/>
            <param name="output-count" value="4"/>
        </box>
        <box name="CalcDLCPULoad" type="query">
            <input port="1" stream="out:Iterate_1"/>
            <output port="1" stream="out:CalcDLCPULoad_1"/>
            <dataref id="querytable" name="WindowData"/>
            <param name="operation" value="read"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="(URI==element)"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-nothing"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none">
                    <include field="element"/>
                </item>
                <item name="current" selection="none"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <expressions>
                    <include field="DLAvgCPUload">avg(CPUload)</include>
                </expressions>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="DelDL2" type="query">
            <input port="1" stream="out:DelDL1_1"/>
            <output port="1" stream="out:DelDL2_1"/>
            <dataref id="querytable" name="WindowData"/>
            <param name="operation" value="delete"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="input.URI == current.URI"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="all"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <item name="current" selection="none"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="IsMarkDL" type="filter">
            <input port="1" stream="out:Split_4"/>
            <output port="1" stream="out:IsMarkDL_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="((Data.Category=='Cluster') &amp;&amp; (Data.Name =='Self'))"/>
        </box>
        <box name="NewNode" type="filter">
            <input port="1" stream="out:Split_2"/>
            <output port="1" stream="out:NewNode_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="CQSScopeTransition==0  &amp;&amp; Data.Category=='Cluster' &amp;&amp; Data.Name=='Member'"/>
        </box>
        <box name="SeeIfNonNull" type="query">
            <input port="1" stream="out:Iterate2_1"/>
            <output port="1" stream="out:SeeIfNonNull_1"/>
            <dataref id="querytable" name="LVNodeInfoTable"/>
            <param name="operation" value="read"/>
            <param name="where" value="expression"/>
            <param name="limit" value="1"/>
            <param name="where-expression" value="input.element == current.URI &amp;&amp; notnull(isDL)"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-nothing"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="all"/>
                <item name="current" selection="none">
                    <include field="isDL"/>
                </item>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="WriteLVNodeInfo" type="query">
            <input port="1" stream="out:Split_3"/>
            <output port="1" stream="out:WriteLVNodeInfo_1"/>
            <dataref id="querytable" name="LVNodeInfoTable"/>
            <param name="operation" value="write"/>
            <param name="where" value="primary-key"/>
            <param name="key-field.0" value="URI"/>
            <param name="key-value.0" value="input.URI"/>
            <param name="key-field.1" value="CQSInternalID"/>
            <param name="key-value.1" value="input.CQSInternalID"/>
            <param name="write-type" value="update"/>
            <param name="if-write-fails" value="force"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none"/>
                <item name="old" selection="none"/>
                <item name="new" selection="all"/>
                <item name="current" selection="none"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
                <expressions>
                    <set field="Category">Data.Category</set>
                    <set field="Name">Data.Name</set>
                    <set field="Detail">Data.Detail</set>
                    <set field="Value">Data.Value</set>
                    <set field="Information">Data.Information</set>
                    <set field="UpdateTime">Data.UpdateTime</set>
                </expressions>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="AlreadyKnown" type="query">
            <input port="1" stream="out:NewNode_1"/>
            <output port="1" stream="out:AlreadyKnown_1"/>
            <dataref id="querytable" name="LVNodeInfoTable"/>
            <param name="operation" value="read"/>
            <param name="where" value="expression"/>
            <param name="limit" value="1"/>
            <param name="where-expression" value="Data.Value == Value &amp;&amp; Category=='Cluster' &amp;&amp; Name=='Member'"/>
            <param name="key-field.0" value="URI"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="all"/>
                <item name="current" prefix="Old_" selection="none">
                    <include field="URI"/>
                </item>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="DLLoadOrGC" type="filter">
            <input port="1" stream="out:WriteLVNodeInfo_1"/>
            <output port="1" stream="out:DLLoadOrGC_1"/>
            <output port="2" stream="out:DLLoadOrGC_2"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="isDL &amp;&amp; Category='System' &amp;&amp; Name=='Cores' &amp;&amp;  Detail=='LoadAvg'"/>
            <param name="expression.1" value="isDL &amp;&amp; Category='JVM' &amp;&amp; Name=='Heap' &amp;&amp;  contains(Detail, 'Old Generation')"/>
        </box>
        <box name="DLTooBusy" type="filter">
            <input port="1" stream="out:CalcDLCPULoad_1"/>
            <output port="1" stream="out:DLTooBusy_1"/>
            <param name="autogen-nomatch-port" value="false"/>
            <param name="expression.0" value="true -- DLAvgCPUload &gt; ${SINGLE_DL_LOAD_THRESHOLD}"/>
        </box>
        <box name="GetAllNullsDL" type="query">
            <input port="1" stream="out:SeeIfNonNull_1"/>
            <output port="1" stream="out:GetAllNullsDL_1"/>
            <dataref id="querytable" name="LVNodeInfoTable"/>
            <param name="operation" value="read"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="input.element == URI &amp;&amp; isnull(current.isDL)"/>
