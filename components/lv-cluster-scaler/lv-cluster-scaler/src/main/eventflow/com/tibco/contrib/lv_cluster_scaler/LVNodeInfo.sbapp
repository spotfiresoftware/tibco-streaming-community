<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<modify version="10.6.2-SNAPSHOT_df0ac623b762291859ff2a8643925b0a916d3d4f">
    <add>
        <annotations>
            <annotation name="hygienic"/>
        </annotations>
        <type-metadata>
            <param name="type" value="module"/>
            <param name="fully-qualified-name" value="com.tibco.contrib.lv_cluster_scaler.LVNodeInfo"/>
        </type-metadata>
        <memory-model-settings/>
        <import from="lvinterfaces.LVNodeInfoSchemas"/>
        <named-schemas>
            <schema name="LVQueryDataSchema">
                <field name="URI" type="string"/>
                <field name="TableName" type="string"/>
                <field name="CQSInternalID" type="long"/>
                <field name="CQSScopeTransition" type="int"/>
                <field name="Snapshot" type="bool"/>
                <field name="NewJSON" type="string"/>
                <field name="OldJSON" type="string"/>
            </schema>
        </named-schemas>
        <parameter-definitions>
            <parameter default="60" name="WINDOW_SIZE_SEC"/>
            <parameter default="2.0" name="SINGLE_DL_LOAD_THRESHOLD"/>
            <parameter default="2.0" name="CLUSTER_LOAD_THRESHOLD"/>
        </parameter-definitions>
        <dynamic-variables/>
        <data name="LVNodeInfoTable" shared="false" source="concrete" type="querytable">
            <param name="storage-method" value="heap"/>
            <param name="replicated" value="false"/>
            <table-schema>
                <schema>
                    <parent-schemas>
                        <parent-schema name="LVNodeInfoSchemas"/>
                    </parent-schemas>
                    <field name="URI" type="string"/>
                    <field name="CQSInternalID" type="long"/>
                    <field name="isDL" type="bool"/>
                </schema>
                <primary-index type="btree">
                    <field name="URI"/>
                    <field name="CQSInternalID"/>
                </primary-index>
                <index type="btree">
                    <field name="URI"/>
                </index>
                <index type="btree">
                    <field name="isDL"/>
                </index>
            </table-schema>
            <param name="truncate" value="false"/>
            <preload format="csv" mode="empty"/>
        </data>
        <data name="WindowData" shared="false" source="concrete" type="querytable">
            <param name="storage-method" value="heap"/>
            <param name="replicated" value="false"/>
            <table-schema>
                <schema>
                    <field name="sequenceNum" type="long"/>
                    <field name="eventTime" type="timestamp"/>
                    <field name="URI" type="string"/>
                    <field name="CPUload" type="double"/>
                    <field name="oldCollections" type="int"/>
                </schema>
                <primary-index type="btree">
                    <field name="sequenceNum"/>
                </primary-index>
                <index type="btree">
                    <field name="eventTime"/>
                </index>
                <index type="btree">
                    <field name="URI"/>
                </index>
            </table-schema>
            <param name="truncate" value="false"/>
            <preload format="csv" mode="empty"/>
        </data>
        <stream name="LVNodeInfoIn">
            <schema>
                <field name="URI" type="string"/>
                <field name="TableName" type="string"/>
                <field name="CQSInternalID" type="long"/>
                <field name="CQSScopeTransition" type="int"/>
                <field name="Snapshot" type="bool"/>
                <field name="NewJSON" type="string"/>
                <field name="OldJSON" type="string"/>
            </schema>
        </stream>
        <stream name="Ping">
            <schema>
                <field name="time" type="timestamp"/>
            </schema>
        </stream>
        <stream name="RegisterNode">
            <schema>
                <field name="URI" type="string"/>
                <field name="IsAdd" type="bool"/>
            </schema>
        </stream>
        <output-stream name="RegisterNode"/>
        <box name="IsAddRemove" type="filter">
            <input port="1" stream="LVNodeInfoIn"/>
            <output port="1" stream="out:IsAddRemove_1"/>
            <output port="2" stream="out:IsAddRemove_2"/>
            <param name="autogen-nomatch-port" value="true"/>
            <param name="expression.0" value="CQSScopeTransition==0 || CQSScopeTransition==2"/>
            <param name="expression.1" value="true"/>
        </box>
        <box name="Split2" type="split">
            <input port="1" stream="Ping"/>
            <output port="1" stream="out:Split2_1"/>
            <output port="2" stream="out:Split2_2"/>
            <output port="3" stream="out:Split2_3"/>
            <output port="4" stream="out:Split2_4"/>
            <output port="5" stream="out:Split2_5"/>
            <param name="output-count" value="5"/>
        </box>
        <box name="CalcClusterCPULoad" type="query">
            <input port="1" stream="out:Split2_3"/>
            <output port="1" stream="out:CalcClusterCPULoad_1"/>
            <dataref id="querytable" name="WindowData"/>
            <param name="operation" value="read"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="notnull(CPUload)"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-nothing"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none"/>
                <item name="current" selection="none"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <expressions>
                    <include field="ClusterAvgCPUload">avg(CPUload)</include>
                </expressions>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="CalcClusterOldGen" type="query">
            <input port="1" stream="out:Split2_4"/>
            <output port="1" stream="out:CalcClusterOldGen_1"/>
            <dataref id="querytable" name="WindowData"/>
            <param name="operation" value="read"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="notnull(oldCollections)"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-nothing"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none"/>
                <item name="current" selection="none"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <expressions>
                    <include field="ClusterOldGenCollections">sum(oldCollections)</include>
                </expressions>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="GetUIRs" type="query">
            <input port="1" stream="out:Split2_2"/>
            <output port="1" stream="out:GetUIRs_1"/>
            <dataref id="querytable" name="WindowData"/>
            <param name="operation" value="read"/>
            <param name="where" value="all-rows"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-nothing"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none"/>
                <item name="current" selection="none"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <expressions>
                    <include field="URIList">unique(URI, 1000)</include>
                </expressions>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="ListAllURI" type="query">
            <input port="1" stream="out:Split2_5"/>
            <output port="1" stream="out:ListAllURI_1"/>
            <dataref id="querytable" name="LVNodeInfoTable"/>
            <param name="operation" value="read"/>
            <param name="where" value="all-rows"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-nothing"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="none"/>
                <item name="current" selection="none"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <expressions>
                    <include field="URIList">unique(URI, 1000)</include>
                </expressions>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="NewLVNodeInfo" type="map">
            <input port="1" stream="out:IsAddRemove_1"/>
            <output port="1" stream="out:NewLVNodeInfo_1"/>
            <target-list>
                <item name="input" selection="all"/>
                <expressions>
                    <include field="Data">parsejson(LVNodeInfoSchemas(), NewJSON)</include>
                </expressions>
            </target-list>
        </box>
        <box name="OldLVNodeInfo" type="map">
            <input port="1" stream="out:IsAddRemove_2"/>
            <output port="1" stream="out:OldLVNodeInfo_1"/>
            <target-list>
                <item name="input" selection="all"/>
                <expressions>
                    <include field="Data">parsejson(LVNodeInfoSchemas(), OldJSON)</include>
                </expressions>
            </target-list>
        </box>
        <box name="TrimWindow" type="query">
            <input port="1" stream="out:Split2_1"/>
            <output port="1" stream="out:TrimWindow_1"/>
            <dataref id="querytable" name="WindowData"/>
            <param name="operation" value="delete"/>
            <param name="where" value="expression"/>
            <param name="where-expression" value="eventTime &lt; now()-seconds(${WINDOW_SIZE_SEC})"/>
            <param name="write-type" value="insert"/>
            <param name="if-write-fails" value="ignore"/>
            <param name="no-match-mode" value="output-no-match-null"/>
            <param name="order-by-direction" value="none"/>
            <target-list>
                <item name="input" selection="all"/>
                <item name="old" selection="none"/>
                <item name="new" selection="none"/>
                <item name="current" selection="none"/>
            </target-list>
            <target-list name="insert">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="update">
                <item name="input" selection="all"/>
            </target-list>
            <target-list name="no-match">
                <item name="input" selection="all"/>
            </target-list>
        </box>
        <box name="ClusterLoadLog" type="outputadapter">
